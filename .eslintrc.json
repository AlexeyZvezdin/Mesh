{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "allowTemplateLiterals": 0,
    "func-names": "off",
    "prefer-const": 1,
    "dot-notation": 1,
    "prefer-destructuring": 1
  }
}
